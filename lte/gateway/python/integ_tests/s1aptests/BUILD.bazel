load("@python_deps//:requirements.bzl", "requirement")
load("@rules_python//python:defs.bzl", "py_library")
load("@rules_pyvenv//:venv.bzl", "py_venv")
load("//bazel:python_test.bzl", "pytest_test")
load(
    "//bazel:test_constants.bzl",
    "TAG_EXTENDED_TEST",
    "TAG_EXTENDED_TEST_SETUP",
    "TAG_EXTENDED_TEST_TEARDOWN",
    "TAG_NON_SANITY_TEST",
    "TAG_NON_SANITY_TEST_SETUP",
    "TAG_NON_SANITY_TEST_TEARDOWN",
    "TAG_PRECOMMIT_TEST",
    "TAG_TRAFFIC_SERVER_TEST",
)

genrule(
    name = "integ_tests_pth",
    outs = ["integ_tests.pth"],
    cmd = "\n".join([
        'echo /home/vagrant/magma/lte/gateway/python >> \"$@\"',
        'echo /home/vagrant/magma/orc8r/gateway/python >> \"$@\"',
    ]),
)

py_library(
    # needs to be wrapped in a py_library so that py_env can consume it
    name = "integ_tests_pth_wrapper",
    data = [":integ_tests_pth"],
    imports = ["."],
)

py_venv(
    # This target creates a Python environment for running integration tests.
    # Usage: `bazel run //path:python_env <path of your choice>`
    # Note that this cannot update an existing Python environment, thus for updating you need
    # to run something like `rm -rf ~/python_env` before recreating the environment.
    # NOTE: until https://github.com/cedarai/rules_pyvenv/issues/1 is resolved, the environment does
    # not cover magma libraries without adaptions - but all external and generated dependencies.
    # The magma source is setup via a .pth-file that points to the magma reporsitory.
    name = "python_env",
    deps = [
        ":gpp_types",
        ":integ_tests_pth_wrapper",
        ":s1ap_wrapper",
        requirement("pytest"),
        requirement("flaky"),
    ],
)

MAGMA_ROOT = "../../../../../"

LTE_ROOT = "{}lte/gateway/python".format(MAGMA_ROOT)

py_library(
    name = "s1ap_utils",
    srcs = [
        "s1ap_utils.py",
    ],
    deps = [
        "//lte/gateway/python/integ_tests/s1aptests/ovs:rest_api",
        "//lte/gateway/python/magma/subscriberdb:sid",
        "//lte/protos:abort_session_python_grpc",
        "//lte/protos:ha_service_python_grpc",
        "//lte/protos:mobilityd_python_proto",
        "//lte/protos:policydb_python_proto",
        "//lte/protos:session_manager_python_grpc",
        "//lte/protos:spgw_service_grpc_proto",
        "//orc8r/protos:directoryd_python_grpc",
        "@s1ap_test_util//:s1ap_types",
